   "source": [
    "# Write a program to reverse an integer in Python.\n",
    "# reverse number\n",
    "n=int(input(\"Enter Number : \"))\n",
    "\n",
    "rev=0\n",
    "\n",
    "while(n>0):\n",
    "    rev=(rev*10)+(n%10)\n",
    "    n=n//10\n",
    "    print(\"The Reverse Number is :\",rev)"
   ]
